<%= simple_form_for(@client) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :cpf %>
    <%= f.input :address %>
    <%= f.input :neighborhood %>
    <%= f.input :city %>
    <%= f.input :stade %>
    <%= f.input :zip %>
    <%= f.input :consultant, :wrapper => :inline_checkbox %>
    <%= f.input :indicator, :wrapper => :inline_checkbox %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, :class => "create" %>
  </div>
<% end %>
